{
  "name": "monolith/component-bootstrapping",
  "description": "Module for bootstrapping components in the Monolith framework.",
  "keywords": [
    "monolith",
    "component bootstrapping"
  ],
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "monolith/dependency-injection": "^7",
    "monolith/collections": "^6"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\ComponentBootstrapping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\ComponentBootstrapping\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
