{
  "name": "monolith/component-bootstrapping",
  "description": "Module for bootstrapping components in the Monolith framework.",
  "keywords": [
    "monolith",
    "component bootstrapping"
  ],
  "require": {
    "php": ">=8.1",
    "monolith/dependency-injection": "^10",
    "monolith/collections": "^10"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\ComponentBootstrapping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\ComponentBootstrapping\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
