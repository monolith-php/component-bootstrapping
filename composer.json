{
  "name": "monolith/component-bootstrapping",
  "description": "Module for bootstrapping components in the Monolith framework.",
  "keywords": [
    "monolith",
    "component bootstrapping"
  ],
  "require": {
    "php": ">=8",
    "monolith/dependency-injection": "^9",
    "monolith/collections": "^9"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.humanmusic.eu",
      "canonical": false,
      "only": [
        "monolith/*"
      ]
    }
  ],
  "autoload": {
    "psr-4": {
      "Monolith\\ComponentBootstrapping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\ComponentBootstrapping\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
